{
    "name": "daodao97/apidog",
    "description": "A swagger library for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "swagger"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "hyperf/http-server": "~2.0.0",
        "hyperf/logger": "~2.0.0",
        "hyperf/validation": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Apidog\\": "src/"
        },
        "files": [
            "./src/function.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Apidog\\ConfigProvider"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17"
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "cs-check": "php-cs-fixer fix $1 --dry-run"
    }
}
